<div class="change-map-contents">
  <div class="d-flex justify-content-around card-wrap">
    <% zones.each do |zone| %>
    <div class="item">
      <%= link_to divesite_path(zone) do %>
        <span>
          <%= image_tag '/images/divesites/airtank.png', class: "diving-icon" %>
          <%= zone.name %><i class="fas fa-angle-right"></i>
        </span>
      <% end %>
    </div>
    <% end %>
  </div>
</div>
